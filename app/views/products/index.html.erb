<%= form_with url: search_products_path, method: :post do |form| %>
<%= form.search_field :title_search, oninput: "this.form.requestSubmit()" %>
<%= form.submit 'DESC', name: "sort", oninput: "this.form.requestSubmit()" %>
<%= form.submit 'ASC', name: "sort", oninput: "this.form.requestSubmit()" %>
<% end %>


<%= render partial: 'search_product',  locals: {products: @products} %>


